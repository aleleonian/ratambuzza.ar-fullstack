<h2>Postales</h2>
<ul>
    <% console.log(postcards)%>
    <% if(postcards.length < 1) {%>
    Todav√≠a no generaste postales, bro.
    <% } else {%>
    <% postcards.forEach(pc => { %>
    <li>
        <%= pc.status %>
        <% if (pc.image_url) { %>
        <img src="<%= pc.image_url %>" width="200">
